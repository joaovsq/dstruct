{
    "name": "dstruct",
    "description": "ORM for D language, similar to Hibernate",
    "authors": ["Vadim Lopatin", "Jo√£o V."],
    "homepage": "https://github.com/joaovsq/dstruct",
    "license": "BSL-1.0",
    "dependencies": {
    },
    "targetType": "staticLibrary",
    "targetPath": "bin",
    "buildRequirements": [
		"allowWarnings"
    ],
    "systemDependencies": "Depending on configuration: ODBC, PostgreSQL and/or SQLite v3",
    "configurations": [
        {
            "name": "PGSQL",
            "versions": ["USE_PGSQL"],
            "libs-posix": ["libpq-dev"],
            "libs-windows": ["libpq"],
            "subConfigurations": {
                "ddbc": "PGSQL"
            }
        },
        {
            "name": "SQLite",
            "versions": ["USE_SQLITE"],
            "libs-posix": ["sqlite3"],
            "libs-windows": ["sqlite3"],
            "copyFiles-windows-x86_64": [ "libs/win64/sqlite3.dll" ]
        }
    ]
}
