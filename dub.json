{
    "name": "dstruct-orm",
    "description": "ORM for D language, similar to Hibernate",
    "authors": [
        "Vadim Lopatin",
        "Jo√£o V."
    ],
    "homepage": "https://github.com/joaovsq/dstruct",
    "license": "BSL-1.0",
    "dependencies": {},
    "targetType": "dynamicLibrary",
    "targetPath": "bin",
    "buildRequirements": [
        "allowWarnings"
    ],
    "systemDependencies": "Depending on configuration: PostgreSQL or SQLite v3",
    "configurations": [
        {
            "name": "pgsql",
            "versions": [
                "USE_PGSQL"
            ],
            "copyFiles-linux-x86_64": [
                "libs/linux64/libpq.so"
            ],
            "copyFiles-windows-x86_64": [
                "libs/win64/libpq.dll"
            ]
        },
        {
            "name": "sqlite",
            "versions": [
                "USE_SQLITE"
            ],
            "libs-posix": [
                "sqlite3"
            ],
            "copyFiles-windows-x86_64": [
                "libs/win64/sqlite3.dll"
            ]
        }
    ]
}